<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="StockApp.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
		</sectionGroup>
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<appSettings>
		<add key="LocalDbPath" value="Local.db" />
		
		<!--console-->
		<add key="serilog:using:Console" value="Serilog.Sinks.Console" />
		<add key="serilog:write-to:Console" />
		
		<!--file-->
		<add key="serilog:using:File" value="Serilog.Sinks.File" />
		<add key="serilog:write-to:File.shared" value="true" />
		<add key="serilog:write-to:File.path" value="Logs\log-.txt" />
		<add key="serilog:write-to:File.rollingInterval" value="Day"/>
		<add key="serilog:write-to:File.retainedFileCountLimit" value="10" />

		<!--seq-->
		<add key="serilog:using:Seq" value="Serilog.Sinks.Seq" />
		<add key="serilog:write-to:Seq.serverUrl" value="http://seq.local:5341" />
	</appSettings>
	<userSettings>
		<StockApp.Properties.Settings>
			<setting name="PriceLimit" serializeAs="String">
				<value>60</value>
			</setting>
			<setting name="ContBonusTimesLimit" serializeAs="String">
				<value>10</value>
			</setting>
			<setting name="SimulateMaxMonth" serializeAs="String">
				<value>6</value>
			</setting>
			<setting name="KDJRange" serializeAs="String">
				<value>2</value>
			</setting>
			<setting name="GoodInfoLogin" serializeAs="String">
				<value>LOGIN=</value>
			</setting>
			<setting name="TraceDays" serializeAs="String">
				<value>32</value>
			</setting>
			<setting name="HoldValueMaxRatio" serializeAs="String">
				<value>0.1</value>
			</setting>
		</StockApp.Properties.Settings>
	</userSettings>
