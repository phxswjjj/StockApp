      </setting>
      <setting name="KDJRange" serializeAs="String">
        <value>2</value>
      </setting>
      <setting name="GoodInfoLogin" serializeAs="String">
        <value>LOGIN=</value>
      </setting>
      <setting name="TraceDays" serializeAs="String">
        <value>32</value>
      </setting>
      <setting name="HoldValueMaxRatio" serializeAs="String">
        <value>0.1</value>
      </setting>
    </StockApp.Properties.Settings>
  </userSettings>
